{
	"name": "string-matchall",
	"version": "0.0.0",
	"description": "Spec-compliant polyfill for String.prototype.matchAll ESnext proposal.",
	"main": "index.js",
	"scripts": {
		"build": "ecmarkup --js=spec.js --css=spec.css spec.emu | js-beautify -f - --type=html -t > spec.html",
		"prepublish": "npm run build",
		"test": "evalmd *.md && es-shim-api --bound && npm run tests-only && npm run lint",
		"tests-only": "npm run test:module && npm run test:shim",
		"test:module": "node test/index.js",
		"test:shim": "node test/shimmed.js",
		"lint": "npm run eslint",
		"eslint": "eslint test/*.js index.js implementation.js polyfill.js shim.js hidden.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ljharb/String.prototype.matchAll.git"
	},
	"keywords": [
		"String.prototype.matchAll",
		"matchAll",
		"match",
		"regex",
		"regexp",
		"regular",
		"expression",
		"matches"
	],
	"author": "Jordan Harband <ljharb@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ljharb/String.prototype.matchAll/issues"
	},
	"homepage": "https://github.com/ljharb/String.prototype.matchAll#readme",
	"dependencies": {
		"es-abstract": "^1.3.1",
		"define-properties": "^1.1.1",
		"function-bind": "^1.0.2",
		"regexp.prototype.flags": "^1.1.1"
	},
	"devDependencies": {
		"foreach": "^2.0.5",
		"ecmarkup": "^2.2.0",
		"es5-shim": "^4.1.13",
		"es6-shim": "^0.33.3",
		"js-beautify": "^1.5.10",
		"object.assign": "^4.0.1",
		"tape": "^4.2.0",
		"@es-shims/api": "^1.0.0",
		"eslint": "^1.5.0",
		"@ljharb/eslint-config": "^1.2.0",
		"evalmd": "^0.0.16"
	}
}
